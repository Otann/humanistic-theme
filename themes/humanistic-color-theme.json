  "tokenColors": [
    {
      "name": "Dim known words",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#738091",
        "fontStyle": ""
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#ABB3BF"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#238551"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#C5CBD3"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#7C327C"
      }
    },
    {
      "name": "Global definitions",
      "scope": "entity.name",
      "settings": {
        "foreground": "#111418"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#738091"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#AC2F33"
      }
    }
  ]
