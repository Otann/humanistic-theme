{
  "name": "humanistic-theme",
  "displayName": "humanistic-theme",
  "description": "Inspired by the light Intellij theme, focused on good contrast and non-invasive ui",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Humanistic Theme",
        "uiTheme": "vs",
        "path": "./themes/Humanistic Theme-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "mustache": "^4.2.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "generate": "node ./src/generate.js",
    "local": "node ./src/local.js",
    "watch": "watch 'npm run local' ./src"
  },
  "type": "module"
}
